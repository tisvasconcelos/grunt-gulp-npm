{
  "name": "grunt-gulp-npm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf assets/build/",
    "uglify": "mkdir -p assets/build/js && cp -a assets/src/js/. assets/build/js/ && find assets/build/js/ -name '*.js' -exec uglifyjs {} -c -m -o {} \\;",
    "sass": "node-sass assets/src/css/ -r --include-path assets/src/css/ --output-style compressed --source-map true -o assets/build/css/",
    "build": "npm run clean && npm run sass && npm run uglify && node npm.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tisvasconcelos/grunt-gulp-npm.git"
  },
  "author": "Tiago Vasconcelos <tisvasconcelos@gmail.com> (https://github.com/tisvasconcelos/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tisvasconcelos/grunt-gulp-npm/issues"
  },
  "homepage": "https://github.com/tisvasconcelos/grunt-gulp-npm#readme",
  "devDependencies": {
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-sass": "1.0.0",
    "grunt-contrib-uglify": "2.0.0",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-sass": "2.3.2",
    "gulp-uglify": "2.0.0",
    "node-sass": "3.8.0",
    "pump": "1.0.1",
    "run-sequence": "1.2.2",
    "uglify-js": "2.7.3"
  }
}
